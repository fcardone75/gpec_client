{
  "name": "bni/gpec",
  "description": "A hello world example package.",
  "type": "library",
  "require": {
    "php": "^8.1",
    "symfony/http-client": "^6.4",
    "symfony/cache": "^6.0",
    "guzzlehttp/guzzle": "^7.9",
    "vlucas/phpdotenv": "^5.6",
    "ramsey/uuid": "^4.7",
    "php-soap/engine": "2.12.0",
    "php-soap/ext-soap-engine": "1.6.0",
    "monolog/monolog": "^3.8"
  },
  "require-dev": {
    "laminas/laminas-code": "^4.14",
    "wsdltophp/packagegenerator": "^4.1"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bni\\Gpec\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload-dev": {
    "psr-4": {
      "Bni\\Gpec\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": false
    }
  }
}
